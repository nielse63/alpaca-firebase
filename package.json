{
  "name": "alpaca-trading-scripts",
  "version": "2.0.0",
  "description": "Algotrading bot example using TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "dotenvx run --env-file=.env.test -- jest",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky || true",
    "start": ".bin/run",
    "restart": ".bin/restart",
    "dev": "ts-node src/index.ts",
    "lint": "eslint --fix . && prettier --write .",
    "prebuild": "rm -rf dist/",
    "build": "tsc",
    "prewatch": "npm run build",
    "watch": "tsc -w",
    "cron": "node dist/crypto/cron.js",
    "crypto:backtest": "node dist/crypto/backtest.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "dev:app": "nodemon ./app/bin/www",
    "start:app": "pm2 start --time --only server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/alpaca-trading-scripts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nielse63/alpaca-trading-scripts/issues"
  },
  "homepage": "https://github.com/nielse63/alpaca-trading-scripts#readme",
  "engines": {
    "node": ">=22.2.0"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.1",
    "cookie-parser": "~1.4.4",
    "date-fns": "^3.6.0",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "firebase": "^10.12.4",
    "fs-extra": "^11.2.0",
    "http-errors": "~1.6.3",
    "lodash.findlastindex": "^4.6.0",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "trading-signals": "^3.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@dotenvx/dotenvx": "^1.6.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash.findlastindex": "^4.6.9",
    "@types/moment-business": "^0.1.6",
    "@types/node": "^20.14.12",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express-generator": "^4.16.1",
    "husky": "^9.1.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.7",
    "moment-business": "^3.0.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{json,md}": "prettier --write --cache",
    "*.{ts,js}": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ]
  }
}
