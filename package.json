{
  "name": "alpaca-firebase",
  "version": "1.0.8",
  "description": "Algotrading bot example using TypeScript",
  "main": "./index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ci": ".bin/ci",
    "test": "jest .",
    "prepare": "husky || true",
    "lint": "eslint --fix --cache . && prettier --write --cache .",
    "preserve": ".bin/set-env",
    "serve": "firebase serve",
    "predeploy": ".bin/set-env",
    "deploy": "firebase deploy",
    "build": ".bin/build",
    "release": ".bin/release",
    "verify": ".bin/verify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielse63/alpaca-firebase.git"
  },
  "keywords": [],
  "author": "Erik Nielsen <erik@312development.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nielse63/alpaca-firebase/issues"
  },
  "homepage": "https://github.com/nielse63/alpaca-firebase#readme",
  "engines": {
    "node": ">=22.12.0"
  },
  "workspaces": [
    "./packages/*"
  ],
  "dependencies": {
    "@alpaca-firebase/account": "file:packages/account",
    "@alpaca-firebase/alpaca": "file:packages/alpaca",
    "@alpaca-firebase/helpers": "file:packages/helpers",
    "@alpaca-firebase/orders": "file:packages/orders",
    "@alpaca-firebase/positions": "file:packages/positions",
    "@alpaca-firebase/quote": "file:packages/quote",
    "dotenv": "^16.4.7",
    "firebase": "^11.1.0"
  },
  "devDependencies": {
    "@nielse63/copy-env": "^1.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-extended": "^2.4.0",
    "eslint-plugin-monorepo": "^0.3.2",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.{json,md,yml,ts}": "prettier --write --cache",
    "*.js": [
      "eslint --fix --cache",
      "prettier --write --cache"
    ],
    "./packages/**/*.{json,js}": ".bin/package-doctor"
  }
}
